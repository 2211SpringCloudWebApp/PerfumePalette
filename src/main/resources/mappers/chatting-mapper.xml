<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ChattingMapper">
	<resultMap type="Chatting" id="chattingResultMap">
		<id property="chatNo" column="CHAT_NO"/>
		<result property="memberId" column="MEMBER_ID"/>
		<result property="memberNickname" column="MEMBER_NICKNAME"/>
		<result property="chatContent" column="CHAT_CONTENT"/>
		<result property="chatDate" column="CHAT_DATE"/>
	</resultMap>
	
	
	
	<!-- 메시지 전송 -->
	<insert id="insertChatting">
		INSERT INTO CHAT_TBL VALUES(SEQ_CHATNO.NEXTVAL, #{memberId}, #{memberNickname}, #{chatContent}, DEFAULT)
	</insert>
</mapper>